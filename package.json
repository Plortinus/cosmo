{
  "name": "cosmo-wc",
  "description": "a shareable web components library with Svelte",
  "version": "0.0.2",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "keywords": [
    "svelte",
    "web-components",
    "library"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Plortinus/cosmo.git"
  },
  "scripts": {
    "build": "rollup -c",
    "build:demo": "rollup -c rollup.demo.config.js",
    "dev": "rollup -c rollup.demo.config.js -w",
    "start": "sirv demo/public",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "rollup": "^2.50.5",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sirv-cli": "^1.0.12",
    "svelte": "^3.38.2"
  },
  "files": [
    "dist"
  ]
}
